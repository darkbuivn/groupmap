@Model 
<div id="googleMap" style="width:100%;height:100%; min-height: 750px;"></div>

<script async defer>
    function initMap() {
        var mapProp= {
            center: new google.maps.LatLng(@ViewBag.LatestCoordinate.Latitude, @ViewBag.LatestCoordinate.Longitude),
            zoom: 9,
        };
        //var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
        var uluru = {lat: @ViewBag.LatestCoordinate.Latitude, lng: @ViewBag.LatestCoordinate.Longitude};
        // The map, centered at Uluru
        map = new google.maps.Map(
            document.getElementById('googleMap'), { zoom: 11, center: uluru });
        
        // The marker, positioned at Uluru
        var marker = new google.maps.Marker({position: uluru, map: map, label: '@User.Identity.Name'});        

        @*var markers = locations.map(function(location, i) {
            return new google.maps.Marker({
                position: location,
                label: '@User.Identity.Name' + i.toString()
            });
        });*@
    }
    var locations = [
        {lat: -31.563910, lng: 147.154312},
        {lat: -33.718234, lng: 150.363181},
        {lat: -33.727111, lng: 150.371124},
        {lat: -33.848588, lng: 151.209834}];
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDbsrf54wbBlOa8ZsfOSDI7RIVzXIvUQqk&callback=initMap"></script>